#instaling rails 4.0.0 in ruby 2.4.1
 
1.rvm use 2.4.1
2.rvm gemset list
3.rvm gemset create rails4.0.0
4.rvm gemset list
5.rvm gemset use rails4.0.0
6.gem install rails -v '4.0.0'
7. ruby -v
8. rails -v

#install rails 4.1.0 in ruby 2.4.1
1. rvm use 2.4.1
2.rvm gemset list 
3. rvm gemset create rails 4.1.0
4. rvm gemset list
5.rvm gemset use rails 4.1.0
6.gem install rails '4.1.0'
7.ruby -v
8.rails -v

